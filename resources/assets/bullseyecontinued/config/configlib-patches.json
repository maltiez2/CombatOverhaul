{
  "version": 3,
  "patches": {
    "string": {
      "game:itemtypes/combatoverhaul/spear.json": {
        "attributes/ThrowAttack/Aiming/CursorType": "spears_aiming_cursor_type",
        "behaviors/1/properties/stats/HitSound": "(arrow_ping_sound_on_hit) ? value : empty_string"
      },
      "combatoverhaul:patches/tools/pickaxe.json": {
        "1/value": "(pickaxe_animation) ? value : vanilla_item"
      },
      "game:itemtypes/combatoverhaul/arrow.json": {
        "behaviors/0/properties/stats/HitSound": "(arrow_ping_sound_on_hit) ? value : empty_string"
      },
      "combatoverhaul:config/settings.json": {
        "BowsAimingCursorType": "bows_aiming_cursor_type"
      }
    },
    "boolean": {
      "combatoverhaul:patches/tools/axe.json": {
        "0/value/RenderingOffset": "not(tool_rendering_offset)",
        "2/value/RenderingOffset": "not(tool_rendering_offset)",
        "0/value/CanSplitLogs": "axe_can_split_logs",
        "2/value/CanSplitLogs": "axe_can_split_logs",
        "1/enabled": "axe_animation",
        "3/enabled": "axe_animation",
        "4/enabled": "axe_animation"
      },
      "combatoverhaul:patches/tools/pickaxe.json": {
        "0/value/RenderingOffset": "not(tool_rendering_offset)"
      }
    },
    "number": {
      "combatoverhaul:config/animations/tool/axe.json": {
        "axe-swing-forward/SoundFrames/0/Volume": "tool_whoosh_sound"
      },
      "combatoverhaul:config/animations/tool/pickaxe.json": {
        "pickaxe-swing-forward-1/SoundFrames/0/Volume": "tool_whoosh_sound",
        "pickaxe-swing-forward-2/SoundFrames/0/Volume": "tool_whoosh_sound",
        "pickaxe-swing-forward-3/SoundFrames/0/Volume": "tool_whoosh_sound"
      },
      "combatoverhaul:entities/spear.json": {
        "client/behaviors/0/airDragFactor": "value / spears_thrown_distance",
        "client/behaviors/0/gravityFactor": "value / spears_thrown_distance",
        "server/behaviors/0/airDragFactor": "value / spears_thrown_distance",
        "server/behaviors/0/gravityFactor": "value / spears_thrown_distance"
      },
      "game:itemtypes/combatoverhaul/bow.json": {
        "attributes/AimingByType/@@*/AimDifficulty": "bow_aiming_difficulty",
        "attributes/ArrowDamageMultiplierByType/@@*": "value * bows_damage_multiplier"
      },
      "combatoverhaul:patches/weapons/spear.json": {
        "1/value/Aiming/AimDifficulty": "spears_aiming_difficulty",
        "3/value/properties/stats/DamageStatsByType/@@*/Damage": "value * spears_range_damage_multiplier"
      },
      "combatoverhaul:config/settings.json": {
        "BowsAimingHorizontalLimit": "value * bow_aiming_horisontal_limit",
        "BowsAimingVerticalLimit": "value * bow_aiming_vertical_limit"
      },
      "combatoverhaul:patches/tools/pickaxe.json": {
        "0/value/AnimationSpeedBonusFromMiningSpeed": "tool_animation_speed_bonus"
      }
    }
  },
  "constants": {
    "empty_string": "",
    "vanilla_axe": "ItemAxe",
    "vanilla_item": "Item"
  },
  "settings": [
    {
      "code": "bow_aiming_difficulty",
      "type": "float",
      "ingui": "combatoverhaul:setting-bow_aiming_difficulty",
      "default": 1.0,
      "range": {
        "min": 0.0,
        "max": 2.0
      }
    },
    {
      "code": "bows_aiming_cursor_type",
      "type": "string",
      "ingui": "combatoverhaul:setting-bows_aiming_cursor_type",
      "clientSide": true,
      "default": "Moving",
      "values": [
        "None",
        "Vanilla",
        "Fixed",
        "Moving"
      ]
    },
    {
      "code": "bow_aiming_vertical_limit",
      "type": "float",
      "ingui": "combatoverhaul:setting-bow_aiming_vertical_limit",
      "clientSide": true,
      "default": 1.0,
      "range": {
        "min": 0.0,
        "max": 2.0
      }
    },
    {
      "code": "bow_aiming_horisontal_limit",
      "type": "float",
      "ingui": "combatoverhaul:setting-bow_aiming_horisontal_limit",
      "clientSide": true,
      "default": 1.0,
      "range": {
        "min": 0.0,
        "max": 2.0
      }
    },
    {
      "code": "spears_aiming_cursor_type",
      "type": "Fixed",
      "ingui": "combatoverhaul:setting-spears_aiming_cursor_type",
      "default": "Fixed",
      "values": [
        "None",
        "Vanilla",
        "Fixed",
        "Moving"
      ]
    },
    {
      "code": "spears_aiming_difficulty",
      "type": "float",
      "ingui": "combatoverhaul:setting-spears_aiming_difficulty",
      "default": 1.0,
      "range": {
        "min": 0.0,
        "max": 2.0
      }
    },
    {
      "code": "spears_thrown_distance",
      "type": "float",
      "ingui": "combatoverhaul:setting-spears_thrown_distance",
      "default": 1.0,
      "range": {
        "min": 0.5,
        "max": 5.0
      }
    },
    {
      "code": "bows_damage_multiplier",
      "type": "float",
      "ingui": "combatoverhaul:setting-bows_damage_multiplier",
      "default": 1.0,
      "logarithmic": true,
      "range": {
        "min": 0.2,
        "max": 5.0
      }
    },
    {
      "code": "spears_range_damage_multiplier",
      "type": "float",
      "ingui": "combatoverhaul:setting-spears_range_damage_multiplier",
      "default": 1.0,
      "logarithmic": true,
      "range": {
        "min": 0.2,
        "max": 5.0
      }
    },
    {
      "code": "axe_animation",
      "type": "boolean",
      "ingui": "combatoverhaul:setting-axe_animation",
      "default": true
    },
    {
      "code": "pickaxe_animation",
      "type": "boolean",
      "ingui": "combatoverhaul:setting-pickaxe_animation",
      "default": true
    },
    {
      "code": "tool_rendering_offset",
      "type": "boolean",
      "ingui": "combatoverhaul:setting-tool_rendering_offset",
      "default": true
    },
    {
      "code": "axe_can_split_logs",
      "type": "boolean",
      "ingui": "combatoverhaul:setting-axe_can_split_logs",
      "default": true
    },
    {
      "code": "arrow_ping_sound_on_hit",
      "type": "boolean",
      "ingui": "combatoverhaul:setting-arrow_ping_sound_on_hit",
      "default": true
    },
    {
      "code": "tool_whoosh_sound",
      "type": "float",
      "ingui": "combatoverhaul:setting-tool_whoosh_sound",
      "default": 0.5,
      "range": {
        "min": 0.0,
        "max": 1.0
      }
    },
    {
      "code": "tool_animation_speed_bonus",
      "type": "float",
      "ingui": "combatoverhaul:setting-tool_animation_speed_bonus",
      "comment": "combatoverhaul:setting-comment-tool_animation_speed_bonus",
      "default": 0.3,
      "range": {
        "min": 0.0,
        "max": 1.5
      }
    }
  ]
}